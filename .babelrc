{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        ["import", { "libraryName": "antd-mobile" }],
        "transform-decorators-legacy",
      ]
    },
    "production": {
      "plugins": [
        ["import", { "libraryName": "antd-mobile" }],
        "transform-decorators-legacy",
      ]
    }
  },

}
